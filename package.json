{
  "name": "seawhale-to-skywhale",
  "version": "1.0.0",
  "description": "CLI tool to migrate data from SonarQube to SonarCloud",
  "main": "src/index.js",
  "bin": {
    "seawhale": "./src/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "transfer": "node src/index.js transfer -c config.json --verbose",
    "validate": "node src/index.js validate -c config.json",
    "test:connection": "node src/index.js test -c config.json",
    "status": "node src/index.js status -c config.json",
    "reset": "node src/index.js reset -c config.json",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "sonarqube",
    "sonarcloud",
    "migration",
    "cli",
    "scanner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "commander": "^12.0.0",
    "form-data": "^4.0.0",
    "protobufjs": "^7.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
