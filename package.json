{
  "name": "cloudvoyager",
  "version": "1.0.0",
  "description": "A CLI tool to migrate all projects from SonarQube Server to SonarCloud.io",
  "main": "src/index.js",
  "bin": {
    "cloudvoyager": "./src/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "transfer": "node src/index.js transfer -c config.json --verbose",
    "transfer:auto-tune": "node src/index.js transfer -c config.json --verbose --auto-tune",
    "validate": "node src/index.js validate -c config.json",
    "test:connection": "node src/index.js test -c config.json",
    "status": "node src/index.js status -c config.json",
    "reset": "node src/index.js reset -c config.json",
    "migrate": "node src/index.js migrate -c migrate-config.json --verbose",
    "migrate:dry-run": "node src/index.js migrate -c migrate-config.json --verbose --dry-run",
    "migrate:skip-hotspot-metadata": "node src/index.js migrate -c migrate-config.json --verbose --skip-hotspot-metadata-sync",
    "migrate:skip-issue-metadata": "node src/index.js migrate -c migrate-config.json --verbose --skip-issue-metadata-sync",
    "migrate:skip-all-metadata": "node src/index.js migrate -c migrate-config.json --verbose --skip-issue-metadata-sync --skip-hotspot-metadata-sync",
    "migrate:auto-tune": "node src/index.js migrate -c migrate-config.json --verbose --auto-tune",
    "migrate:skip-all-metadata:auto-tune": "node src/index.js migrate -c migrate-config.json --verbose --skip-issue-metadata-sync --skip-hotspot-metadata-sync --auto-tune",
    "migrate:skip-quality-profiles": "node src/index.js migrate -c migrate-config.json --verbose --skip-quality-profile-sync",
    "migrate:skip-quality-profiles:auto-tune": "node src/index.js migrate -c migrate-config.json --verbose --skip-quality-profile-sync --auto-tune",
    "migrate:skip-all": "node src/index.js migrate -c migrate-config.json --verbose --skip-issue-metadata-sync --skip-hotspot-metadata-sync --skip-quality-profile-sync --auto-tune",
    "migrate:only-scan-data": "node src/index.js migrate -c migrate-config.json --verbose --only scan-data",
    "migrate:only-scan-data-all-branches": "node src/index.js migrate -c migrate-config.json --verbose --only scan-data-all-branches",
    "migrate:only-quality-gates": "node src/index.js migrate -c migrate-config.json --verbose --only quality-gates",
    "migrate:only-quality-profiles": "node src/index.js migrate -c migrate-config.json --verbose --only quality-profiles",
    "migrate:only-permissions": "node src/index.js migrate -c migrate-config.json --verbose --only permissions",
    "migrate:only-permission-templates": "node src/index.js migrate -c migrate-config.json --verbose --only permission-templates",
    "migrate:only-portfolios": "node src/index.js migrate -c migrate-config.json --verbose --only portfolios",
    "migrate:only-issue-metadata": "node src/index.js migrate -c migrate-config.json --verbose --only issue-metadata",
    "migrate:only-hotspot-metadata": "node src/index.js migrate -c migrate-config.json --verbose --only hotspot-metadata",
    "migrate:only-project-settings": "node src/index.js migrate -c migrate-config.json --verbose --only project-settings",
    "sync-metadata": "node src/index.js sync-metadata -c migrate-config.json --verbose",
    "sync-metadata:auto-tune": "node src/index.js sync-metadata -c migrate-config.json --verbose --auto-tune",
    "sync-metadata:skip-issue-metadata": "node src/index.js sync-metadata -c migrate-config.json --verbose --skip-issue-metadata-sync",
    "sync-metadata:skip-hotspot-metadata": "node src/index.js sync-metadata -c migrate-config.json --verbose --skip-hotspot-metadata-sync",
    "sync-metadata:skip-quality-profiles": "node src/index.js sync-metadata -c migrate-config.json --verbose --skip-quality-profile-sync",
    "test": "c8 --reporter=lcov --reporter=text --report-dir=coverage ava",
    "test:fast": "ava",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "build": "node scripts/build.js",
    "package": "node scripts/build.js --package",
    "package:bun": "node scripts/build.js --bun",
    "package:bun:cross": "node scripts/build.js --bun --cross"
  },
  "keywords": [
    "sonarqube",
    "sonarcloud",
    "migration",
    "cli",
    "scanner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.6.0",
    "commander": "^12.0.0",
    "form-data": "^4.0.0",
    "pdfmake": "^0.3.4",
    "protobufjs": "^7.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "ava": "^6.4.1",
    "c8": "^10.1.3",
    "esbuild": "^0.27.3",
    "eslint": "^8.56.0",
    "esmock": "^2.7.3",
    "postject": "^1.0.0-alpha.6",
    "sinon": "^21.0.1"
  },
  "optionalDependencies": {
    "bun": "^1.3.9"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "timeout": "60s",
    "concurrency": 4,
    "nodeArguments": [
      "--no-warnings",
      "--loader=esmock"
    ]
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/protobuf/schema/**"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
