{
  "sonarqube": {
    "url": "https://your-sonarqube-server.com",
    "token": "sqp_your_sonarqube_admin_token"
  },
  "sonarcloud": {
    "enterprise": {
      "key": "your-enterprise-key"
    },
    "organizations": [
      {
        "key": "org-one",
        "token": "your_sonarcloud_token_for_org_one",
        "url": "https://sonarcloud.io"
      },
      {
        "key": "org-two",
        "token": "your_sonarcloud_token_for_org_two",
        "url": "https://sonarcloud.io"
      }
    ]
  },
  "transfer": {
    "mode": "full",
    "batchSize": 100
  },
  "migrate": {
    "outputDir": "./migration-output",
    "skipIssueMetadataSync": false,
    "skipHotspotMetadataSync": false,
    "skipQualityProfileSync": false,
    "dryRun": false
  },
  "rateLimit": {
    "maxRetries": 5,
    "baseDelay": 1000,
    "minRequestInterval": 150
  },
  "performance": {
    "autoTune": false,
    "maxConcurrency": 8,
    "maxMemoryMB": 0,
    "sourceExtraction": { "concurrency": 10 },
    "hotspotExtraction": { "concurrency": 10 },
    "issueSync": { "concurrency": 5 },
    "hotspotSync": { "concurrency": 3 },
    "projectMigration": { "concurrency": 1 }
  }
}
